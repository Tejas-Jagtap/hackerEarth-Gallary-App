<body>

    <div class="container">
        <h2>Image Name: <%= data.imgName%>
        </h2>
        <img class="card-img-top" src="<%= data.imgUrl%>" alt="Card image cap" width="70%" height="400">
        <h3>Image URL: <%= data.imgUrl%>
        </h3>
        <h3>Image Details: <%= data.imgDetails%>
        </h3>
        <div class="col-sm m-3 d-flex justify-content-between">
            <a class="btn btn-primary" href="/<%= data._id%>/edit" role="button">Edit</a>
            <a class="btn btn-danger" href="/delete/<%= data._id%>" role="button">Delete</a>
        </div>

    </div>





